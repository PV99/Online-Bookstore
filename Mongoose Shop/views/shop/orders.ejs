<%- include('../includes/head.ejs') %>
<link rel="stylesheet" href="/css/product.css">
</head>

<body>
    <%- include('../includes/navigation.ejs') %>
    <main>
        <% if (orders.length <= 0) { %>
        <h1>Nothing there!</h1>
        <% } else { %>
        <ul>
            <% orders.forEach(order => { %>
            <li>
                <h1># <%= order._id %> </h1>
                <ul>
                    <% order.products.forEach(item => { %>
                    <li class="rowList">
                        <h3 class="spacedP"> <%= item.product.title %> </h3>
                        <h3 class="spacedP"> Quantity: <%= item.quantity %> </h3>

                        <% }) %>
                </ul>
            <li>
                <% }) %>

        </ul>
        <% } %>
    </main>
    <%- include('../includes/end.ejs') %>